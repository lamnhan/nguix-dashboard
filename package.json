{
  "name": "@lamnhan/nguix-dashboard",
  "version": "0.0.0",
  "description": "An Angular UI kit for content management.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:lib": "ng build dashboard && cpy README.md LICENSE dist/dashboard",
    "build:app": "ng build",
    "build": "npm run build:app && npm run build:lib",
    "deploy:lib": "cd ./dist/dashboard && npm publish && cd ../../",
    "deploy:app": "git push",
    "deploy": "npm run deploy:app && npm run deploy:lib",
    "test": "ng test dashboard",
    "lint": "ng lint dashboard",
    "e2e": "ng e2e",
    "coverage": "cat ./coverage/*/lcov.info | coveralls",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37"
  },
  "keywords": [
    "lamnhan",
    "angular",
    "ui",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lamnhan/nguix-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/lamnhan/nguix-dashboard/issues"
  },
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@lamnhan/ngx-useful": "0.0.7",
    "@lamnhan/unistylus": "0.0.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "coveralls": "^3.1.0",
    "cpy-cli": "^3.1.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "typescript": "~4.2.3"
  }
}
